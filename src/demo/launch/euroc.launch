<launch>
    <arg name="config_path" default = "$(find demo)/config/euroc/euroc_config.yaml" />
	  <arg name="vins_path" default = "$(find demo)/../" />
    
    <node name="feature_tracker" pkg="feature_tracker" type="feature_tracker" output="log">
        <param name="config_file" type="string" value="$(arg config_path)" />
        <param name="vins_folder" type="string" value="$(arg vins_path)" />
    </node>

    <node name="vins_estimator" pkg="vins_estimator" type="vins_estimator" output="screen">
       <param name="config_file" type="string" value="$(arg config_path)" />
       <param name="vins_folder" type="string" value="$(arg vins_path)" />
    </node>

    <node name="rvizvisualisation" pkg="rviz" type="rviz" output="log" args="-d $(find demo)/config/vins_rviz_config.rviz" />
    
    <arg name="bagfile_path" default = "/home/dwq/Dataset_euroc/MH_01_easy.bag" />
    <node name="rosbag_play" pkg="rosbag" type="play" args="$(arg bagfile_path)"  />

</launch>
